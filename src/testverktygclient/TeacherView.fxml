<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1000.0" styleClass="white" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testverktygclient.TeacherController">
   <children>
      <ChoiceBox fx:id="studentsBox" layoutX="229.0" layoutY="174.0" prefHeight="25.0" prefWidth="347.0" />
      <TableView fx:id="completedColumn" layoutX="229.0" layoutY="250.0" prefHeight="200.0" prefWidth="493.0">
        <columns>
        <!-- this.courseName = courseCode; -->
        <!-- this.TestName = TestName; -->
        <!-- this.userScore = userScore; -->
        <!-- this.testMaxScore = testMaxScore; -->
          <TableColumn fx:id="courseColumn" prefWidth="133.0" resizable="false" text="Course Name" />
          <TableColumn fx:id="testColumn" prefWidth="202.0" resizable="false" text="Test Name" />
          <TableColumn fx:id="scoreColumn" prefWidth="87.0" resizable="false" text="Score" />
          <TableColumn fx:id="maxScoreColumn" prefWidth="69.0" resizable="false" text="Max Score" />
        </columns>
      </TableView>
      <Button fx:id="createTestButton" layoutX="613.0" layoutY="174.0" mnemonicParsing="false" onAction="#spawnNewWindow" prefHeight="27.0" prefWidth="109.0" styleClass="orange-button" stylesheets="@style.css" text="Create New Test">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <AnchorPane prefHeight="80.0" prefWidth="1000.0" styleClass="headerfooter" stylesheets="@taketestview.css">
         <children>
            <Label fx:id="loggedInAsLabel" layoutX="658.0" layoutY="14.0" prefHeight="42.0" prefWidth="251.0" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Button layoutX="904.0" layoutY="24.0" mnemonicParsing="false" onAction="#signOut" style="-fx-background-color: rgb(221, 72, 20);" text="Sign out" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Label layoutX="8.0" layoutY="4.0" prefHeight="70.0" prefWidth="287.0" text="Newton Testing Tool 2017" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children></AnchorPane>
      <Button layoutX="403.0" layoutY="538.0" mnemonicParsing="false" onAction="#spawnStatistics" styleClass="orange-button" stylesheets="@style.css" text="Show test statistics" />
   </children>
</AnchorPane>
