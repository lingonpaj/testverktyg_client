<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1000.0" styleClass="anchorwhite" stylesheets="@taketestview.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testverktygclient.FXMLTakeTestViewController">
   <children>
      <AnchorPane fx:id="question_pane" layoutX="355.0" layoutY="80.0" prefHeight="620.0" prefWidth="639.0" styleClass="anchorwhite" stylesheets="@taketestview.css">
         <children>
            <Label fx:id="question_text" layoutX="26.0" layoutY="94.0" prefHeight="57.0" prefWidth="589.0" textOverrun="CLIP" wrapText="true">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <VBox fx:id="option_list" layoutX="34.0" layoutY="162.0" prefHeight="272.0" prefWidth="574.0" />
            <Label fx:id="question_number" layoutX="26.0" layoutY="14.0" prefHeight="80.0" prefWidth="469.0">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
            <Button fx:id="finishbutton" layoutX="479.0" layoutY="523.0" mnemonicParsing="false" onAction="#finishTest" prefHeight="47.0" prefWidth="119.0" styleClass="finishbutton" stylesheets="@taketestview.css" text="Finish" />
            <Label fx:id="timerLabel" layoutX="432.0" layoutY="37.0" prefHeight="57.0" prefWidth="213.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="80.0" prefWidth="1000.0" styleClass="headerfooter" stylesheets="@taketestview.css">
         <children>
            <Label fx:id="question_name" alignment="CENTER" layoutX="297.0" layoutY="2.0" prefHeight="80.0" prefWidth="459.0" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font size="46.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <VBox layoutX="14.0" layoutY="80.0" prefHeight="620.0" prefWidth="335.0">
         <children>
            <VBox fx:id="question_list" prefHeight="425.0" prefWidth="295.0">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="30.0" />
               </VBox.margin>
            </VBox>
            <HBox prefHeight="64.0" prefWidth="335.0">
               <children>
                  <Button fx:id="previous_button" mnemonicParsing="false" prefHeight="74.0" prefWidth="139.0" styleClass="nextpreviousbutton" stylesheets="@taketestview.css" text="≪">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                     <font>
                        <Font size="44.0" />
                     </font>
                  </Button>
                  <Button fx:id="next_button" mnemonicParsing="false" prefHeight="74.0" prefWidth="139.0" styleClass="nextpreviousbutton" stylesheets="@taketestview.css" text="≫">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin>
                     <font>
                        <Font size="44.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="2.0" top="45.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
